<div class="container">
	<div class="row">
		<div id="map-outer" class="col-md-12">
			<div class="col-md-6 col-md-offset-3">

				<table class="table table-striped">
					<thead>
						<tr>
							<th><font size="5"><strong>Locations</strong></font></th>
						</tr>
					</thead>

					<tbody>
						<% @entries.each do |location| %>
						<tr>
							<td><%= link_to location.name, location %></td>
						</tr>
						<% end %>

					</tbody>
				</table>
			</div>						<div align="center">
							<%= will_paginate @entries, renderer: BootstrapPagination::Rails %>
						</div>
		</div>
	</div>
</div>
