<div class="row col-md-offset-1">

<h1>Listing counties</h1>

	<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @counties.each do |county| %>
      <tr>
        <td><%= link_to county.name, county %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>