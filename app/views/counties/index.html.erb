<h1>Listing counties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @counties.each do |county| %>
      <tr>
        <td><%= county.name %></td>
        <td><%= link_to 'Show', county %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>